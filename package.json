{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:vite": "vite",
    "dev:vercel": "vercel dev",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json && tsc --noEmit -p tsconfig.api.json",
    "typecheck:api": "tsc --noEmit -p tsconfig.api.json",
    "test": "npm run typecheck  && eslint . && vitest",
    "test:no-typecheck": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "npm run typecheck && vitest --coverage --run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:ci": "playwright test --project=chromium",
    "supabase:migrate": "node scripts/migrate-supabase-project.js",
    "supabase:link": "node scripts/link-and-push.js",
    "supabase:push": "./bin/supabase db push",
    "supabase:pull": "./bin/supabase db push --include-all",
    "supabase:reset": "./bin/supabase db pull"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.0.0",
    "@clerk/themes": "^2.4.30",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@runwayml/sdk": "^3.4.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/query-sync-storage-persister": "^5.90.7",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-persist-client": "^5.90.7",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/jszip": "^3.4.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.24",
    "jszip": "^3.10.1",
    "lucide-react": "^0.543.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "replicate": "^1.3.0",
    "tailwind-merge": "^2.2.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vercel/node": "^5.4.1",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "express": "^5.1.0",
    "globals": "^15.9.0",
    "jsdom": "^27.0.1",
    "postcss": "^8.4.35",
    "serve": "^14.2.5",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-bundle-analyzer": "^1.2.3",
    "vitest": "^3.2.4"
  }
}
